{
    "rust-client.disableRustup": true,
    "snippets.userSnippetsDirectory" : "~/.config/nvim/my-snippets/UltiSnips",
    "languageserver": {
        "ccls": {
            "command": "ccls",
            "filetypes": ["c", "cc", "cpp", "c++", "objc", "objcpp"],
            "rootPatterns": [".ccls", "compile_commands.json", ".git/", ".hg/"],
            "trace.server": "verbose",
            "initializationOptions": {
                "request": {
                    "timeout": 5000
                },
                "cache": {
                    "directory": ".ccls-cache"
                },
                "clang": {
                    "extraArgs": ["-fms-extensions", "-fms-compatibility", "-fdelayed-template-parsing"],
                    "excludeArgs": []
                }
            }
        }
    }
}
